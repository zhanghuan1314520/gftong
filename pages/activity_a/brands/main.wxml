<view class="{{[ 'dev-activity _div data-v-56c0acba',[show_pop_box?'devhide':''] ]}}">
    <view class="dev-banner _div data-v-56c0acba">
        <view class="top-tools _div data-v-56c0acba">
            <view bindtap="__e" class="rules _div data-v-56c0acba" data-event-opts="{{[ [ 'tap',[ [ 'tapRules',['$event'] ] ] ] ]}}">
                <text class="data-v-56c0acba">评选</text>
                <text class="data-v-56c0acba">规则</text>
            </view>
            <button class="share-wrap data-v-56c0acba" openType="share">
                <fc-icon bind:__l="__l" class="data-v-56c0acba" type="48_share_white" vueId="3bad4622-1"></fc-icon>
                <text class="tx data-v-56c0acba">分享</text>
            </button>
        </view>
        <text class="activity-time data-v-56c0acba">数据统计时间: 2020年1月1日00:00-11月30日23:59</text>
        <view class="{{[ 'tabs _div data-v-56c0acba',[tabs_fixed_top?'fixed':''] ]}}">
