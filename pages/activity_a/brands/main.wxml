        </view>
    </view>
    <view class="statement _div data-v-56c0acba">{{tips}}</view>
    <pop-box bind:__l="__l" bind:hide="__e" class="data-v-56c0acba" data-event-opts="{{[ [ '^hide',[ ['hide'] ] ] ]}}" rules="{{rules}}" show="{{show_pop_box}}" vueId="3bad4622-4"></pop-box>
